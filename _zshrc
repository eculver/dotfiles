# Path to your oh-my-zsh configuration.
export ZSH=$HOME/.oh-my-zsh

# Set to the name theme to load.
# Look in ~/.oh-my-zsh/themes/
export ZSH_THEME="gozilla"

# Set to this to use case-sensitive completion
# export CASE_SENSITIVE="true"

# Comment this out to disable weekly auto-update checks
# export DISABLE_AUTO_UPDATE="true"

# Uncomment following line if you want to disable colors in ls
# export DISABLE_LS_COLORS="true"

# Which plugins would you like to load? (plugins can be found in ~/.oh-my-zsh/plugins/*)
# Example format: plugins=(rails git textmate ruby lighthouse)
plugins=(git git-flow svn brew github osx)

source $ZSH/oh-my-zsh.sh

# Customize to your needs...

# WD/dev aliases
source ~/.zshrc_wd

# Path
PATH=$PATH:~/.gem/ruby/1.8/bin:/usr/local/Cellar/python/2.7.1/bin:~/bin:~/local/node/bin
export PATH

# Virtualenv
export WORKON_HOME=~/.virtualenvs
export VIRTUALENVWRAPPER_PYTHON=/usr/local/Cellar/python/2.7.1/bin/python
source /usr/local/bin/virtualenvwrapper.sh

# Python
export PYTHONSTARTUP=$HOME/.pythonrc.py

# NPM completion
source ~/local/node/lib/node_modules/npm/lib/utils/completion.sh

alias chrome_ipad='open /Applications/Google\ Chrome.app --args -user-agent="Mozilla/5.0(iPad; U; CPU iPhone OS 3_2 like Mac OS X; en-us) AppleWebKit/531.21.10 (KHTML, like Gecko) Version/4.0.4 Mobile/7B314 Safari/531.21.10"'
alias update_db_widget='cp /Volumes/eculver/projects/wd-app/resources/widget/draggableBackground/js/draggable-background.js gallery-draggable-background/js'
alias start_memcached='memcached -m 64 -p 11211 -u `whoami` -l 127.0.0.1'
alias gousc='cd ~/dev/usc ; workon usc'
alias goufogrid='cd ~/dev/ufogrid ; workon ufogrid'
alias gototb='cd ~/dev/totb ; workon totb'
alias godio='cd ~/dev/dio ; workon dio'
alias goeio='cd ~/dev/eculver.io ; workon eio'
alias gojute='cd ~/dev/JUTE/backend/nodejute'
alias godpp='cd ~/dev/dpp ; workon dpp'
alias gotamoshunas='cd ~/dev/tamoshunas ; workon tamoshunas'

alias pwgen='nocorrect pwgen'
alias ack='nocorrect ack'
